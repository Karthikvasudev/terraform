[
	{
		"name": "nginx",
		"image": "nginx:alpine",
		"essential": true,
		"portMappings": [
			{
				"containerPort": 80,
				"hostPort": 8006
			}
		],
		"links": [
			"crowd"
		],
		"mountPoints": [
			{
				"sourceVolume": "nginx-config",
				"containerPath": "/etc/nginx/nginx.conf"
			}
		],
		"logConfiguration": {
			"logDriver": "awslogs",
			"options": {
				"awslogs-group": "aip-adm-crowd-logs",
				"awslogs-region": "us-east-1"
				}
			},
		"memory": 256,
		"cpu": 100
	},
	{
		"name": "crowd",
		"image": "blacklabelops/crowd:2.9.1",
		"links": [
			"postgres"
		],
		"mountPoints": [
			{
				"sourceVolume": "app-volume",
				"containerPath": "/var/atlassian/crowd"
			}
		],
		"environment": [
			{
				"name" : "CROWD_PROXY_NAME",
				"value" : "crowd.aipadmin.com"
			},
			{
				"name" : "CROWD_PROXY_PORT",
				"value" : "443"
			},
			{
				"name" : "CROWD_PROXY_SCHEME",
				"value" : "https"
			},
			{
				"name" : "CROWD_URL",
				"value" : "https://crowd.aipadmin.com/crowd"
			},
			{
				"name" : "LOGIN_BASE_URL",
				"value" : "https://crowd.aipadmin.com"
			}
		],
		"user": "root",
		"logConfiguration": {
			"logDriver": "awslogs",
			"options": {
				"awslogs-group": "aip-adm-crowd-logs",
				"awslogs-region": "us-east-1"
				}
			},
		"cpu": 512,
		"memory": 2048,
		"essential": true
	},
	{
		"name": "postgres",
		"image": "postgres:9.3",
		"essential": true,
		"environment": [
			{
				"name" : "POSTGRES_USER",
				"value" : "pgadmin"
			},
			{
				"name" : "POSTGRES_PASSWORD",
				"value" : "password"
			},
			{
				"name" : "PGDATA",
				"value" : "/var/lib/postgresql/data/pgdata"
			},
			{
				"name" : "POSTGRES_DB",
				"value" : "crowddb"
			}
		],
		"mountPoints": [
			{
				"sourceVolume": "app-volume",
				"containerPath": "/var/lib/postgresql/data"
			}
		],
		"logConfiguration": {
			"logDriver": "awslogs",
			"options": {
					"awslogs-group": "aip-adm-crowd-logs",
					"awslogs-region": "us-east-1"
				}
			},
		"memory": 750,
		"cpu": 100
	},
	{
		"name": "dockup-pgdump",
		"image": "661072482170.dkr.ecr.us-east-1.amazonaws.com/add/atlassian-dockup-pgdump:latest",
		"essential": true,
		"links": [
			"postgres"
		],
		"environment": [
			{
				"name" : "PGDUMP_OPTIONS",
				"value" : "-Fc --no-acl --no-owner"
			},
			{
				"name" : "PGDUMP_DATABASE",
				"value" : "crowddb"
			},
			{
				"name" : "POSTGRES_ENV_POSTGRES_USER",
				"value" : "pgadmin"
			},
			{
				"name" : "POSTGRES_ENV_POSTGRES_PASSWORD",
				"value" : "password"
			},
			{
				"name" : "POSTGRES_PORT_5432_TCP_ADDR",
				"value" : "postgres"
			},
			{
				"name" : "POSTGRES_PORT_5432_TCP_PORT",
				"value" : "5432"
			},
			{
				"name" : "AWS_BUCKET",
				"value" : "aip-adm-s3-app-data-us-east-1-661072482170"
			},
			{
				"name" : "PREFIX",
				"value" : "backups/adm/pgdump/crowd"
			},
			{
				"name" : "CRON_TIME",
				"value" : "00 5 * * *"
			}
		],
		"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-group": "aip-adm-crowd-logs",
					"awslogs-region": "us-east-1"
				}
			},
		"memory": 256,
		"cpu": 100
	},
	{
		"name": "dockeup-data",
		"image": "661072482170.dkr.ecr.us-east-1.amazonaws.com/add/atlassian-dockup-data:latest",
		"essential": true,
		"mountPoints": [
			{
				"sourceVolume": "app-volume",
				"containerPath": "/opt/aip/aip-adm-crowd"
			}
		],
		"environment": [
			{
				"name" : "BACKUP_NAME",
				"value" : "crowd"
			},
			{
				"name" : "BACKUP_TAR_OPTION",
				"value" : "/opt/aip/aip-adm-crowd/*"
			},
			{
				"name" : "AWS_REGION",
				"value" : "us-east-1"
			},
			{
				"name" : "AWS_BUCKET",
				"value" : "aip-adm-s3-app-data-us-east-1-661072482170"
			},
			{
				"name" : "PREFIX",
				"value" : "backups/adm/data/crowd"
			},
			{
				"name" : "CRON_TIME",
				"value" : "00 5 * * *"
			}
		],
		"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-group": "aip-adm-crowd-logs",
					"awslogs-region": "us-east-1"
				}
			},
		"memory": 256,
		"cpu": 100
	}
]