[
	{
		"name": "nginx",
		"image": "nginx:alpine",
		"essential": true,
		"portMappings": [
			{
				"containerPort": 80,
				"hostPort": 8007
			}
		],
		"mountPoints": [
			{
				"sourceVolume": "nginx-config",
				"containerPath": "/etc/nginx/nginx.conf"
			}
		],
		"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-group": "aip-adm-bio-logs",
					"awslogs-region": "us-east-1"
				}
			},
		"memory": 256,
		"cpu": 100
	}
]
