{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Admin VPC - Stack aip-vpc-admin-us-east-1",
  "Resources": {
    "vpcAdmin": {
      "Type": "AWS::EC2::VPC",
      "Properties": {
        "CidrBlock": "10.85.0.0/21",
        "InstanceTenancy": "default",
        "EnableDnsSupport": "true",
        "EnableDnsHostnames": "false",
        "Tags": [
          {
            "Key": "Name",
            "Value": "aip-vpc-admin-us-east-1"
          }
        ]
      }
    },
    "subnetAdmin": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "CidrBlock": "10.85.0.0/25",
        "AvailabilityZone": "us-east-1a",
        "VpcId": {
          "Ref": "vpcAdmin"
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": "aip-admin-sn-us-east-1a"
          }
        ]
      }
    },
    "doptAbbottPark": {
      "Type": "AWS::EC2::DHCPOptions",
      "Properties": {
        "DomainName": "ec2.internal",
        "DomainNameServers": [
          "AmazonProvidedDNS"
        ]
      }
    },
    "naclAdmin": {
      "Type": "AWS::EC2::NetworkAcl",
      "Properties": {
        "VpcId": {
          "Ref": "vpcAdmin"
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": "aip-admin-nacl-us-east-1"
          }
        ]
      }
    },
    "rtb1f1abe7b": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "VpcId": {
          "Ref": "vpcAdmin"
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": "aip-admin-rtb-us-east-1"
          }
        ]
      }
    },
    "acl5": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "CidrBlock": "0.0.0.0/0",
        "Egress": "true",
        "Protocol": "-1",
        "RuleAction": "allow",
        "RuleNumber": "100",
        "NetworkAclId": {
          "Ref": "naclAdmin"
        }
      }
    },
    "acl6": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "CidrBlock": "0.0.0.0/0",
        "Protocol": "-1",
        "RuleAction": "allow",
        "RuleNumber": "100",
        "NetworkAclId": {
          "Ref": "naclAdmin"
        }
      }
    },
    "subnetacl3": {
      "Type": "AWS::EC2::SubnetNetworkAclAssociation",
      "Properties": {
        "NetworkAclId": {
          "Ref": "naclAdmin"
        },
        "SubnetId": {
          "Ref": "subnetAdmin"
        }
      }
    },
    "subnetroute5": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "RouteTableId": {
          "Ref": "rtb1f1abe7b"
        },
        "SubnetId": {
          "Ref": "subnetAdmin"
        }
      }
    },
    "dchpassoc3": {
      "Type": "AWS::EC2::VPCDHCPOptionsAssociation",
      "Properties": {
        "VpcId": {
          "Ref": "vpcAdmin"
        },
        "DhcpOptionsId": {
          "Ref": "doptAbbottPark"
        }
      }
    }
  }
}