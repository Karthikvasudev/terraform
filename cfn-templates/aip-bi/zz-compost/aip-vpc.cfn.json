{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "This template defines a VPC for an environment (dev, test, qa, prod,...)",
  
  "Parameters": {
  
    "Environment": {
      "Description": "name of the VPC Environment (dev, test, qa, prod)",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "AdminVpcId": {
      "Description": "Administration Vpc Id",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "AdminVpcCIDR": {
      "Description": "Administration Vpc CIDR block assignment",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "AdminVpcPrivateRouteTableId": {
      "Description": "Administration Vpc Route Table Id for Private Subnets",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "KeyName": {
      "Description": "Name of an existing EC2 KeyPair to enable SSH access all hosts",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255",
      "AllowedPattern": "[\\x20-\\x7E]*",
      "ConstraintDescription": "can contain only ASCII characters."
    },
    "VpcName": {
      "Description": "name of the VPC",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcAvailabilityZone1": {
      "Description": "One of two Availability Zones that will be used to create subnets.",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcAvailabilityZone2": {
      "Description": "One of two Availability Zones that will be used to create subnets.",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcNetworkCIDR": {
      "Description": "VPC CidrBlock",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet1Name": {
      "Description": "Subnet name",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet1CIDR": {
      "Description": "Subnet CidrBlock",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet2Name": {
      "Description": "Subnet name",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet2CIDR": {
      "Description": "Subnet CidrBlock",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet3Name": {
      "Description": "Subnet name",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet3CIDR": {
      "Description": "Subnet CidrBlock",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet4Name": {
      "Description": "Subnet name",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet4CIDR": {
      "Description": "Subnet CidrBlock",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet5Name": {
      "Description": "Subnet name",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet5CIDR": {
      "Description": "Subnet CidrBlock",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet6Name": {
      "Description": "Subnet name",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet6CIDR": {
      "Description": "Subnet CidrBlock",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet7Name": {
      "Description": "Subnet name",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet7CIDR": {
      "Description": "Subnet CidrBlock",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet8Name": {
      "Description": "Subnet name",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },
    "VpcSubnet8CIDR": {
      "Description": "Subnet CidrBlock",
      "Type": "String",
      "MinLength": "1",
      "MaxLength": "255"
    },

    "NatInstanceType": {
      "Description": "NAT Device EC2 instance type",
      "Type": "String",
      "Default": "m3.medium",
      "AllowedValues": [
        "t1.micro",
        "m1.small",
        "m1.medium",
        "m1.large",
        "m1.xlarge",
        "m2.xlarge",
        "m2.2xlarge",
        "m2.4xlarge",
        "m3.medium",
        "m3.xlarge",
        "m3.2xlarge",
        "c1.medium",
        "c1.xlarge",
        "cc1.4xlarge",
        "cc2.8xlarge",
        "cg1.4xlarge"
      ],
      "ConstraintDescription": "must be a valid EC2 instance type."
    },
    "NatImageId": {
      "Description": "NAT AMI Id",
      "Type": "String",
      "Default": "ami-c02b04a8",
      "ConstraintDescription": "must be a valid EC2 AMI Id"
    }
  },

  "Mappings": {
    "AWSNATAMI": {
      "us-east-1": {
        "AMI": "ami-c02b04a8"
      },
      "us-west-2": {
        "AMI": "ami-52ff7262"
      },
      "us-west-1": {
        "AMI": "ami-3bcc9e7e"
      },
      "eu-west-1": {
        "AMI": "ami-0b5b6c7f"
      },
      "ap-southeast-1": {
        "AMI": "ami-02eb9350"
      },
      "ap-southeast-2": {
        "AMI": "ami-ab990e91"
      },
      "ap-northeast-1": {
        "AMI": "ami-14d86d15"
      },
      "sa-east-1": {
        "AMI": "ami-0439e619"
      }
    },
    "AWSInstanceType2Arch": {
      "t1.micro": {
        "Arch": "64"
      },
      "m1.small": {
        "Arch": "64"
      },
      "m1.medium": {
        "Arch": "64"
      },
      "m1.large": {
        "Arch": "64"
      },
      "m1.xlarge": {
        "Arch": "64"
      },
      "m2.xlarge": {
        "Arch": "64"
      },
      "m2.2xlarge": {
        "Arch": "64"
      },
      "m2.4xlarge": {
        "Arch": "64"
      },
      "m3.medium": {
        "Arch": "64"
      },
      "m3.xlarge": {
        "Arch": "64"
      },
      "m3.2xlarge": {
        "Arch": "64"
      },
      "c1.medium": {
        "Arch": "64"
      },
      "c1.xlarge": {
        "Arch": "64"
      },
      "cc1.4xlarge": {
        "Arch": "64Cluster"
      },
      "cc2.8xlarge": {
        "Arch": "64Cluster"
      },
      "cg1.4xlarge": {
        "Arch": "64GPU"
      }
    },
    "AWSRegionArch2AMI": {
      "us-east-1": {
        "32": "ami-a0cd60c9",
        "64": "ami-aecd60c7",
        "64Cluster": "ami-a8cd60c1",
        "64GPU": "ami-eccf6285"
      },
      "us-west-2": {
        "32": "ami-46da5576",
        "64": "ami-48da5578",
        "64Cluster": "NOT_YET_SUPPORTED",
        "64GPU": "NOT_YET_SUPPORTED"
      },
      "us-west-1": {
        "32": "ami-7d4c6938",
        "64": "ami-734c6936",
        "64Cluster": "NOT_YET_SUPPORTED",
        "64GPU": "NOT_YET_SUPPORTED"
      },
      "eu-west-1": {
        "32": "ami-61555115",
        "64": "ami-6d555119",
        "64Cluster": "ami-67555113",
        "64GPU": "NOT_YET_SUPPORTED"
      },
      "ap-southeast-1": {
        "32": "ami-220b4a70",
        "64": "ami-3c0b4a6e",
        "64Cluster": "NOT_YET_SUPPORTED",
        "64GPU": "NOT_YET_SUPPORTED"
      },
      "ap-southeast-2": {
        "32": "ami-b3990e89",
        "64": "ami-bd990e87",
        "64Cluster": "NOT_YET_SUPPORTED",
        "64GPU": "NOT_YET_SUPPORTED"
      },
      "ap-northeast-1": {
        "32": "ami-2a19aa2b",
        "64": "ami-2819aa29",
        "64Cluster": "NOT_YET_SUPPORTED",
        "64GPU": "NOT_YET_SUPPORTED"
      },
      "sa-east-1": {
        "32": "ami-f836e8e5",
        "64": "ami-fe36e8e3",
        "64Cluster": "NOT_YET_SUPPORTED",
        "64GPU": "NOT_YET_SUPPORTED"
      }
    }
  },


  "Resources": {
    "Vpc": {
      "Type": "AWS::EC2::VPC",
      "Properties": {
        "CidrBlock": { "Ref": "VpcNetworkCIDR" },
        "EnableDnsSupport" : "true",
        "EnableDnsHostnames" : "true",
        "Tags": [
          {"Key": "Name", "Value": { "Ref": "VpcName" }},
          {"Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },

    "VpcSubnet1": {
      "Type": "AWS::EC2::Subnet",
      "Metadata" : { "Comment" : "Public Subnet"},
      "Properties": {
        "VpcId": {
          "Ref": "Vpc"
        },
        "CidrBlock": {
          "Ref": "VpcSubnet1CIDR"
        },
        "AvailabilityZone": {
          "Ref": "VpcAvailabilityZone1"
        },
        "Tags": [
          { "Key": "Name", "Value": { "Ref": "VpcSubnet1Name" }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },
    "VpcSubnet2": {
      "Type": "AWS::EC2::Subnet",
      "Metadata" : { "Comment" : "Private Subnet"},
      "Properties": {
        "VpcId": {
          "Ref": "Vpc"
        },
        "CidrBlock": {
          "Ref": "VpcSubnet2CIDR"
        },
        "AvailabilityZone": {
          "Ref": "VpcAvailabilityZone1"
        },
        "Tags": [
          { "Key": "Name", "Value": { "Ref": "VpcSubnet2Name" }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },
    "VpcSubnet3": {
      "Type": "AWS::EC2::Subnet",
      "Metadata" : { "Comment" : "Private Subnet"},
      "Properties": {
        "VpcId": {
          "Ref": "Vpc"
        },
        "CidrBlock": {
          "Ref": "VpcSubnet3CIDR"
        },
        "AvailabilityZone": {
          "Ref": "VpcAvailabilityZone1"
        },
        "Tags": [
          { "Key": "Name", "Value": { "Ref": "VpcSubnet3Name" }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },
    "VpcSubnet4": {
      "Type": "AWS::EC2::Subnet",
      "Metadata" : { "Comment" : "Private Subnet"},
      "Properties": {
        "VpcId": {
          "Ref": "Vpc"
        },
        "CidrBlock": {
          "Ref": "VpcSubnet4CIDR"
        },
        "AvailabilityZone": {
          "Ref": "VpcAvailabilityZone1"
        },
        "Tags": [
          { "Key": "Name", "Value": { "Ref": "VpcSubnet4Name" }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },

    "VpcSubnet5": {
      "Type": "AWS::EC2::Subnet",
      "Metadata" : { "Comment" : "Public Subnet"},
      "Properties": {
        "VpcId": {
          "Ref": "Vpc"
        },
        "CidrBlock": {
          "Ref": "VpcSubnet5CIDR"
        },
        "AvailabilityZone": {
          "Ref": "VpcAvailabilityZone2"
        },
        "Tags": [
          {"Key": "Name", "Value": { "Ref": "VpcSubnet5Name" }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },
    "VpcSubnet6": {
      "Type": "AWS::EC2::Subnet",
      "Metadata" : { "Comment" : "Private Subnet"},
      "Properties": {
        "VpcId": {
          "Ref": "Vpc"
        },
        "CidrBlock": {
          "Ref": "VpcSubnet6CIDR"
        },
        "AvailabilityZone": {
          "Ref": "VpcAvailabilityZone2"
        },
        "Tags": [
          { "Key": "Name", "Value": { "Ref": "VpcSubnet6Name" }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },
    "VpcSubnet7": {
      "Type": "AWS::EC2::Subnet",
      "Metadata" : { "Comment" : "Private Subnet"},
      "Properties": {
        "VpcId": {
          "Ref": "Vpc"
        },
        "CidrBlock": {
          "Ref": "VpcSubnet7CIDR"
        },
        "AvailabilityZone": {
          "Ref": "VpcAvailabilityZone2"
        },
        "Tags": [
          { "Key": "Name", "Value": { "Ref": "VpcSubnet7Name" }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },
    "VpcSubnet8": {
      "Type": "AWS::EC2::Subnet",
      "Metadata" : { "Comment" : "Private Subnet"},
      "Properties": {
        "VpcId": {
          "Ref": "Vpc"
        },
        "CidrBlock": {
          "Ref": "VpcSubnet8CIDR"
        },
        "AvailabilityZone": {
          "Ref": "VpcAvailabilityZone2"
        },
        "Tags": [
          { "Key": "Name", "Value": { "Ref": "VpcSubnet8Name" }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },


    "InternetGateway": {
      "Type": "AWS::EC2::InternetGateway",
      "Properties": {
        "Tags": [
          {"Key": "Name", "Value": { "Fn::Join": ["", ["aip-", { "Ref": "Environment" }, "-igw"]] }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },
    "GatewayToInternet": {
      "Type": "AWS::EC2::VPCGatewayAttachment",
      "Properties": {
        "VpcId": {
          "Ref": "Vpc"
        },
        "InternetGatewayId": {
          "Ref": "InternetGateway"
        }
      }
    },

    "NatInstance": {
      "Type": "AWS::EC2::Instance",
      "Properties": {
        "ImageId": {"Ref": "NatImageId"},
        "InstanceType": {"Ref": "NatInstanceType"},
        "KeyName": {"Ref": "KeyName"},
        "Monitoring": "false",
        "AvailabilityZone": {
          "Ref": "VpcAvailabilityZone1"
        },
        "SubnetId": {
          "Ref": "VpcSubnet1"
        },
        "SourceDestCheck": "false",
        "SecurityGroupIds": [
          {
            "Ref": "SgNat"
          }
        ],
        "Tags": [
          { "Key": "Name", "Value": { "Fn::Join": ["", ["aip-", { "Ref": "Environment" }, "-nat"]] }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },
    "NatInstanceEIP": {
      "Type": "AWS::EC2::EIP",
      "Properties": {
        "InstanceId": {
          "Ref": "NatInstance"
        },
        "Domain": "vpc"
      }
    },
    "SgPrivate": {
      "Type": "AWS::EC2::SecurityGroup",
      "Properties": {
        "GroupDescription": "Private Security Group",
        "VpcId": {
          "Ref": "Vpc"
        },
        "SecurityGroupIngress": [
          {
            "IpProtocol": "tcp",
            "FromPort": "443",
            "ToPort": "443",
            "SourceSecurityGroupId": {
              "Ref": "SgNat"
            }
          },
          {
            "IpProtocol": "tcp",
            "FromPort": "80",
            "ToPort": "80",
            "SourceSecurityGroupId": {
              "Ref": "SgNat"
            }
          },
          {
            "IpProtocol": "icmp",
            "FromPort": "-1",
            "ToPort": "-1",
            "CidrIp": "10.0.0.0/8"
          },
          {
            "IpProtocol": "tcp",
            "FromPort": "22",
            "ToPort": "22",
            "CidrIp": "10.0.0.0/8"
          },
          {
            "IpProtocol": "tcp",
            "FromPort": "22",
            "ToPort": "22",
            "CidrIp": "10.85.0.0/25"
          }
        ],
        "SecurityGroupEgress": [
          {
            "IpProtocol": "icmp",
            "FromPort": "-1",
            "ToPort": "-1",
            "DestinationSecurityGroupId": {
              "Ref": "SgNat"
            }
          },
          {
            "IpProtocol": "-1",
            "DestinationSecurityGroupId": {
              "Ref": "SgNat"
            }
          },
          {
            "IpProtocol": "tcp",
            "FromPort": "22",
            "ToPort": "22",
            "CidrIp": "0.0.0.0/0"
          }
        ],
        "Tags": [
          { "Key": "Name", "Value": { "Fn::Join": ["", ["aip-", { "Ref": "Environment" }, "-sg-private"]] }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },
    "SgNat": {
      "Type": "AWS::EC2::SecurityGroup",
      "Properties": {
        "GroupDescription": "VPC NAT Security Group",
        "VpcId": {
          "Ref": "Vpc"
        },
        "SecurityGroupIngress": [
          {
            "IpProtocol": "icmp",
            "FromPort": "-1",
            "ToPort": "-1",
            "CidrIp": "10.85.0.128/25"
          },
          {
            "IpProtocol": "icmp",
            "FromPort": "-1",
            "ToPort": "-1",
            "CidrIp": "10.0.0.0/8"
          },
          {
            "IpProtocol": "tcp",
            "FromPort": "22",
            "ToPort": "22",
            "CidrIp": "10.0.0.0/8"
          },
          {
            "IpProtocol": "tcp",
            "FromPort": "22",
            "ToPort": "22",
            "CidrIp": "10.85.0.128/25"
          }
        ],
        "SecurityGroupEgress": [
          {
            "IpProtocol": "icmp",
            "FromPort": "-1",
            "ToPort": "-1",
            "CidrIp": "0.0.0.0/0"
          },
          {
            "IpProtocol": "tcp",
            "FromPort": "443",
            "ToPort": "443",
            "CidrIp": "0.0.0.0/0"
          },
          {
            "IpProtocol": "tcp",
            "FromPort": "80",
            "ToPort": "80",
            "CidrIp": "0.0.0.0/0"
          },
          {
            "IpProtocol": "tcp",
            "FromPort": "22",
            "ToPort": "22",
            "CidrIp": "0.0.0.0/0"
          }
        ],
        "Tags": [
          { "Key": "Name", "Value": { "Fn::Join": ["", ["aip-", { "Ref": "Environment" }, "-sg-nat"]] }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },
    "SgNatInboundRule1": {
      "Type": "AWS::EC2::SecurityGroupIngress",
      "Properties": {
        "IpProtocol": "tcp",
        "FromPort": "80",
        "ToPort": "80",
        "SourceSecurityGroupId": {
          "Fn::GetAtt": [
            "SgPrivate",
            "GroupId"
          ]
        },
        "GroupId": {
          "Fn::GetAtt": [
            "SgNat",
            "GroupId"
          ]
        }
      }
    },
    "SgNatInboundRule2": {
      "Type": "AWS::EC2::SecurityGroupIngress",
      "Properties": {
        "IpProtocol": "tcp",
        "FromPort": "443",
        "ToPort": "443",
        "SourceSecurityGroupId": {
          "Fn::GetAtt": [
            "SgPrivate",
            "GroupId"
          ]
        },
        "GroupId": {
          "Fn::GetAtt": [
            "SgNat",
            "GroupId"
          ]
        }
      }
    },
    "RouteTablePrivateSubnets": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "VpcId": { "Ref": "Vpc" },
        "Tags": [
          { "Key": "Name", "Value": { "Fn::Join": ["", ["aip-", { "Ref": "Environment" }, 
                        "-rtb-private-", { "Ref": "AWS::Region" }]] }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },
    "RouteTablePublicSubnet": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "VpcId": { "Ref": "Vpc" },
        "Tags": [
          { "Key": "Name", "Value": { "Fn::Join": ["", ["aip-", { "Ref": "Environment" }, 
                        "-rtb-public-", { "Ref": "AWS::Region" }]] }},
          { "Key": "Environment", "Value": { "Ref": "Environment" }}
        ]
      }
    },

    "VpcSubnet1RouteAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "RouteTableId": { "Ref": "RouteTablePublicSubnet" },
        "SubnetId": { "Ref": "VpcSubnet1" }
      }
    },
    "VpcSubnet2RouteAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "RouteTableId": { "Ref": "RouteTablePrivateSubnets" },
        "SubnetId": { "Ref": "VpcSubnet2" }
      }
    },
    "VpcSubnet3RouteAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "RouteTableId": { "Ref": "RouteTablePrivateSubnets" },
        "SubnetId": { "Ref": "VpcSubnet3" }
      }
    },    
    "VpcSubnet4RouteAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "RouteTableId": { "Ref": "RouteTablePrivateSubnets" },
        "SubnetId": { "Ref": "VpcSubnet4" }
      }
    },
    "VpcSubnet5RouteAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "RouteTableId": { "Ref": "RouteTablePublicSubnet" },
        "SubnetId": { "Ref": "VpcSubnet5" }
      }
    },
    "VpcSubnet6RouteAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "RouteTableId": { "Ref": "RouteTablePrivateSubnets" },
        "SubnetId": { "Ref": "VpcSubnet6" }
      }
    },
    "VpcSubnet7RouteAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "RouteTableId": { "Ref": "RouteTablePrivateSubnets" },
        "SubnetId": { "Ref": "VpcSubnet7" }
      }
    },    
    "VpcSubnet8outeAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "RouteTableId": { "Ref": "RouteTablePrivateSubnets" },
        "SubnetId": { "Ref": "VpcSubnet8" }
      }
    },

    "route1": {
      "Type": "AWS::EC2::Route",
      "Metadata" : { "Comment" : "Add a route the private subnets to the NAT"},
      "Properties": {
        "DestinationCidrBlock": "0.0.0.0/0",
        "RouteTableId": { "Ref": "RouteTablePrivateSubnets" },
